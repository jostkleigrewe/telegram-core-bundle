<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">

    <!-- services -->
    <services>

        <!-- TelegramService -->
        <service id="Jostkleigrewe\TelegramCoreBundle\Service\TelegramService" class="Jostkleigrewe\TelegramCoreBundle\Service\TelegramService"  public="true" autowire="true">
        </service>

        <!-- TelegramWebhookService -->
        <service id="Jostkleigrewe\TelegramCoreBundle\Service\TelegramWebhookService" class="Jostkleigrewe\TelegramCoreBundle\Service\TelegramWebhookService"  public="true" autowire="true">
        </service>

        <!-- TelegramLogWebhookRepository -->
        <service id="Jostkleigrewe\TelegramCoreBundle\Repository\TelegramLogWebhookRepository" class="Jostkleigrewe\TelegramCoreBundle\Repository\TelegramLogWebhookRepository"  public="true" autowire="true">
            <tag name="doctrine.repository_service" />
        </service>

    </services>
</container>
